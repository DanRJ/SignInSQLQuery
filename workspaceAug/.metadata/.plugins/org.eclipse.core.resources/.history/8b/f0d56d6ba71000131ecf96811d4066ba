package myfirstjdbc;

import java.sql.*;

public class MyFirstConnection {
	public static void main(String[] args) {
		String brukernavn = "sivpal12";
		String passord = "99564148";
		String url = "jdbc:mysql://localhost/localhostDB";
	
		try {
			Connection con = DriverManager.getConnection(
					url,
					brukernavn,
					passord);	
			System.out.println("This works!");
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
